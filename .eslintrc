{
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "react-app",
    "airbnb"
  ],
  "rules": {
    "jsx-quotes": [
      "error",
      "prefer-single"
    ],
    "import/prefer-default-export": "off",
    "arrow-parens": "off",
    "react/jsx-filename-extension": "warn",
    "react/no-array-index-key": "off",
    "jsx-a11y/no-static-element-interactions": "off",
    "jsx-a11y/interactive-supports-focus": "off",
    "no-nested-ternary": "off",
    "no-plusplus": "off"
  }
}
